{
  "name": "vscode-markdown-ide",
  "displayName": "Markdown IDE",
  "version": "0.1.0",
  "description": "IDE-grade tooling for Markdown: refactoring and autocompletion",
  "publisher": "kevgo",
  "contributes": {
    "configuration": {
      "title": "Markdown IDE",
      "properties": {
        "markdownIDE.autocompleteTitleRegex": {
          "type": "string",
          "description": "A regular expression that captures the part of linked document titles to use as the link title"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevgo/vscode-markdown-ide"
  },
  "bugs": {
    "url": "https://github.com/kevgo/vscode-markdown-ide/issues"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "documentation/tiki_head_square.jpg",
  "keywords": [
    "markdown",
    "development",
    "refactoring",
    "autocompletion"
  ],
  "badges": [
    {
      "url": "https://circleci.com/gh/kevgo/vscode-markdown-ide.svg?style=shield",
      "href": "https://circleci.com/gh/kevgo/vscode-markdown-ide",
      "description": "build status"
    }
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "license": "ISC",
  "scripts": {
    "vscode:prepublish": "make build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "8.2.1",
    "@types/node": "14.14.35",
    "@types/vscode": "1.54.0",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "assert-no-diff": "3.0.6",
    "eslint": "7.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-typescript-sort-keys": "1.5.0",
    "mocha": "8.3.2",
    "prettier": "2.2.1",
    "text-runner": "5.0.0",
    "textrun-make": "0.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "vsce": "1.87.0"
  }
}
